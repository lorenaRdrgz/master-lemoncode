{
    "compilerOptions": {
        "esModuleInterop": true,
        "isolatedModules": true,
        "lib": [
            "ESNext",
            "DOM"
        ],
        "jsx": "react-jsx",
        "module": "ESNext",
        "moduleResolution": "Node",
        "noEmit": true,
        "noImplicitAny": false,
        "noImplicitReturns": true,
        "resolveJsonModule": true,
        "skipLibCheck": true,
        "noEmitOnError": true,
        "sourceMap": true,
        "target": "ESNext",
        "useDefineForClassFields": true
    },
    "include": [
        "src"
    ]
}